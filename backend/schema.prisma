generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://Lester@localhost:5432/fsa-34abc"
}

model User {
  id       Int      @id @default(autoincrement())
  username String   @unique
  password String
}

model Student {
  id    Int    @id @default(autoincrement())
  name  String
  age   Int
  grade String
}